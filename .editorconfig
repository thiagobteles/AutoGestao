[*.cs]

# CS8604: Possível argumento de referência nula.
dotnet_diagnostic.CS8604.severity = none

# CS8602: Desreferência de uma referência possivelmente nula.
dotnet_diagnostic.CS8602.severity = none

# CS8619: A anulabilidade de tipos de referência no valor não corresponde ao tipo de destino.
dotnet_diagnostic.CS8619.severity = none

# CS8603: Possível retorno de referência nula.
dotnet_diagnostic.CS8603.severity = none

# CS8629: O tipo de valor de nulidade pode ser nulo.
dotnet_diagnostic.CS8629.severity = none

# CS8618: O campo não anulável precisa conter um valor não nulo ao sair do construtor. Considere adicionar o modificador "obrigatório" ou declarar como anulável.
dotnet_diagnostic.CS8618.severity = none

# CA1861: Evite matrizes constantes como argumentos
dotnet_diagnostic.CA1861.severity = none

# CS8601: Possível atribuição de referência nula.
dotnet_diagnostic.CS8601.severity = none

# CS8600: Conversão de literal nula ou possível valor nulo em tipo não anulável.
dotnet_diagnostic.CS8600.severity = none

# CS8605: Executando a conversão unboxing de um valor possivelmente nulo.
dotnet_diagnostic.CS8605.severity = none
